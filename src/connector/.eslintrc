{
	"rules": {
		"array-callback-return": 0,
		"block-scoped-var":0,
		"consistent-return": 0,
		"no-console": 0,
		"no-nested-ternary": 0,
		"no-underscore-dangle": 0,
		"no-var": 0,
		"no-void": 0,
		"import/extensions":0,
		"import/order":0,
		"import/prefer-default-export": 0,
		"jsx-a11y/anchor-is-valid": 0,
		"no-shadow": 0,
		"one-var": 0,
		"react/destructuring-assignment": 0,
		"react/jsx-filename-extension": 0,
		"react/no-array-index-key": 0,
		"react/no-multi-comp": 0,
		"react/no-unused-state": 0,
		"react/jsx-uses-react": 1,
		"react/require-default-props": 0,
		"react/prop-types": 0,
		"strict": 0,
		"vars-on-top": 0
},
	"extends": [
		"airbnb",
		"prettier",
		"prettier/react"
	],
	"plugins": [
		"react",
		"prettier"
	],
	"parser": "babel-eslint",
	"parserOptions": {
	"ecmaVersion": 6,
	"sourceType": "module",
		"ecmaFeatures": {
		"jsx": true
		}
	},
	"env": {
	"es6": true,
	"browser": true,
	"node": true
	}
}

